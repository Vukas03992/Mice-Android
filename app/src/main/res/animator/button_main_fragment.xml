<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <objectAnimator
            android:duration="0"
            android:propertyName="translationY"
            android:valueTo="-2dp"
            android:valueType="floatType"/>
    </item>
    <item>
        <objectAnimator
            android:duration="0"
            android:propertyName="translationY"
            android:valueTo="0dp"
            android:valueType="floatType"/>
    </item>
</selector>